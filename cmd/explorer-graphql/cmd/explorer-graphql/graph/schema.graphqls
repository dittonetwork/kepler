scalar Time

type PageInfoInt {
  nextCursor: Int!
  hasNextPage: Boolean!
}

type Block {
  height: Int!
  chainId: String!
  createdAt: Time!
}

type BlockPaginated {
  edges: [Block]!
  pageInfo: PageInfoInt!
}

type Query {
  block(height: Int!): Block
  blocks(cursor: Int, limit: Int!): BlockPaginated
}
